
[
    {
      "name": "rabbitmq",
      "image": "${image}",
      "portMappings": [
        {
          "containerPort": 5672,
          "hostPort": 5672
        }
      ],
      "memory": 300,
      "networkMode": "awsvpc",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "${log_group}",
          "awslogs-region": "${region}",
          "awslogs-stream-prefix": "${log_prefix}"
        }
      },
      "environment": []
    }
  ]
  